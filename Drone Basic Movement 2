from djitellopy import Tello
from time import sleep


tello = Tello()
tello.connect()
print(tello.get_battery())

tello.takeoff()
tello.send_rc_control(0, 50, 0, 0)
sleep(2)

tello.send_rc_control(50, 0, 0, 0)
sleep(2)

tello.send_rc_control(0, -50, 0, 0)
sleep(2)

tello.send_rc_control(-50, 0, 0, 0)
sleep(2) 

tello.land()
